{
    "version": "2",
    "sql": [
        {
            "engine": "postgresql",
            "schema": "internal/repository/schemas/",
            "queries": "internal/repository/queries/",
            "gen": {
                "go": {
                    "out": "models",
                    "emit_json_tags": true,
                    "overrides": [
                        {
                            "db_type": "pg_catalog.varchar",
                            "nullable": true,
                            "go_type": {
                                "type": "string",
                                "pointer": true
                            }
                        },
                        {
                            "db_type": "pg_catalog.timestamptz",
                            "nullable": true,
                            "go_type": {
                                "import": "time",
                                "type": "Time",
                                "pointer": true
                            }
                        },
                        {
                            "db_type": "pg_catalog.inet",
                            "nullable": true,
                            "go_type": {
                                "type": "string",
                                "pointer": true
                            }
                        }
                    ]
                }
            }
        }
    ]
}